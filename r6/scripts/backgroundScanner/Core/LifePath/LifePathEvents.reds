public static func UpbringingEvents(
    POS_OUTCOME_WGT: Int32, 
    NEU_OUTCOME_WGT: Int32, 
    NEG_OUTCOME_WGT: Int32, 
    CORPO_MOD: Int32, 
    NOMAD_MOD: Int32, 
    JUNKIE_MOD: Int32,
    GANGER_MOD: Int32
) -> array<ref<KdspLifePathEvent>> {
    let events: array<ref<KdspLifePathEvent>>;
    PushWeightedLifeEvent(events, KdspCrowdScannerEvents.LVG_MOT_FAT(), POS_OUTCOME_WGT);
    PushWeightedLifeEvent(events, KdspCrowdScannerEvents.ADPT_LVG_HOM(), POS_OUTCOME_WGT);
    PushWeightedLifeEvent(events, KdspCrowdScannerEvents.LVG_GPRNTS(), POS_OUTCOME_WGT);
    PushWeightedLifeEvent(events, KdspCrowdScannerEvents.LVG_RLTIVS(), POS_OUTCOME_WGT);
    PushWeightedLifeEvent(events, KdspCrowdScannerEvents.TGT_NOMAD(), POS_OUTCOME_WGT + NOMAD_MOD);
    PushWeightedLifeEvent(events, KdspCrowdScannerEvents.TGT_MGTOWR(), POS_OUTCOME_WGT);
    PushWeightedLifeEvent(events, KdspCrowdScannerEvents.ADPT_CRP_BRD(), POS_OUTCOME_WGT + CORPO_MOD);
    PushWeightedLifeEvent(events, KdspCrowdScannerEvents.UNCR_PRNTS(), NEU_OUTCOME_WGT);
    PushWeightedLifeEvent(events, KdspCrowdScannerEvents.ORD_HOME(), NEU_OUTCOME_WGT);
    PushWeightedLifeEvent(events, KdspCrowdScannerEvents.HLCPT_PRNTS(), NEU_OUTCOME_WGT);
    PushWeightedLifeEvent(events, KdspCrowdScannerEvents.RGH_NOMAD(), NEU_OUTCOME_WGT + NOMAD_MOD);
    PushWeightedLifeEvent(events, KdspCrowdScannerEvents.UNWLNG_GPRNTS(), NEU_OUTCOME_WGT);
    PushWeightedLifeEvent(events, KdspCrowdScannerEvents.UNWLNG_RLTIVS(), NEU_OUTCOME_WGT);
